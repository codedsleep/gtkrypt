<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-gtkrypt">
    <comment>gtkrypt encrypted file</comment>
    <glob pattern="*.gtkrypt"/>
    <magic priority="50">
      <match type="string" value="GTKRYPT\0" offset="0"/>
    </magic>
    <icon name="application-x-gtkrypt"/>
  </mime-type>
</mime-info>
